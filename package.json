{
  "name": "myzod",
  "version": "0.0.29",
  "description": "",
  "main": "./libs/index.js",
  "types": "./libs/index.d.ts",
  "scripts": {
    "test": "nyc mocha \"test/**/*test.ts\"",
    "build": "rm -rf ./libs && tsc",
    "pub": "npm t && npm run build && npm publish",
    "bench": "ts-node test/benchmark.ts | tee ./test/results.json"
  },
  "keywords": [
    "typescript",
    "schema",
    "validation",
    "type",
    "inference"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidmdm/myzod.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "benchmark": "^2.1.4",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "timeout": 5000,
    "exit": true
  }
}
